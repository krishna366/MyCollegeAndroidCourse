<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.engg.mycollege5"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.engg.mycollege5.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.engg.mycollege5.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.engg.mycollege5.BookmarksActivity"
            android:label="@string/title_activity_bookmarks" >
        </activity>
        <activity
            android:name="com.engg.mycollege5.NewsListActivity"
            android:label="@string/title_activity_news_list" >
        </activity>
        <activity
            android:name="com.engg.mycollege5.PostNewsActivity"
            android:label="@string/title_activity_post_news" >
        </activity>
        <activity
            android:name="com.engg.mycollege5.NewBookmarkActivity"
            android:label="@string/title_activity_new_bookmark" >
        </activity>
        <activity
            android:name="com.engg.mycollege5.CoursesActivity"
            android:label="@string/title_activity_courses" >
        </activity>
        <activity
            android:name="com.engg.mycollege5.CollegeInfoActivity"
            android:label="@string/title_activity_college_info" >
        </activity>
    </application>

</manifest>
